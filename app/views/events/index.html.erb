<h1>hello</h1>

<div class="other-container">
  <% @events.each do |event| %>
    <div class="events-card" data-aos="flip-down" data-aos-duration=90>
      <% if event.photo.attached? %>
        <%= cl_image_tag event.photo.key, height: 150, width: 200, crop: :fill %>
      <% end %>
      <li class="name">
        <p><%=event.name%></p>
      </li>
      <li class="info">
        <p><%=event.description%></p>
      </li>
      <li class="info">
        <p><strong>LOCATION: </strong><%=event.location%></p>
      </li>
      <li class="info">
        <p><strong>Price: </strong><%=event.price%></p>
      </li>
      <%# <%=link_to "MORE INFO", event_path(event)%>
    </div>

  <% end %>
</div>
